@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}



<div class="container col-xxl-12">
    <div class="row flex-lg-row-reverse align-items-center pt-4 g-5">
        <div class="col-10 col-lg-6 d-none d-lg-block">
            <img src="https://mindweb.com.ph/wp-content/uploads/revslider/home-biz/Mindweb_icon.png" class="d-block mx-lg-auto img-fluid" alt="Bootstrap Themes"  loading="lazy">
        </div>
        <div class="col-lg-6 pt-sm-5">
            <h1 class="display-5 fw-bold lh-1 mb-3">Mindweb eSolutions - .NET Core Template</h1>
            <p class="lead">Quickly design and customize responsive mobile-first sites with Bootstrap, the world’s most popular front-end open source toolkit, featuring Sass variables and mixins, responsive grid system, extensive prebuilt components, and powerful JavaScript plugins.</p>
            <div class="d-grid gap-2 d-md-flex justify-content-md-start">
                <button type="button" class="btn btn-primary btn-lg px-4 me-md-2">Primary</button>
                <button type="button" class="btn btn-outline-success btn-lg px-4">Default</button>
            </div>
        </div>
    </div>
</div>

<section id="clients" class="clients mt-5">
    <div class="container">

        <div class="row">

            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center aos-init aos-animate" data-aos="zoom-in" data-aos-delay="100">
                <img src="https://bootstrapmade.com/demo/templates/Bethany/assets/img/clients/client-1.png" class="img-fluid" alt="">
            </div>

            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center aos-init aos-animate" data-aos="zoom-in" data-aos-delay="200">
                <img src="https://bootstrapmade.com/demo/templates/Bethany/assets/img/clients/client-2.png" class="img-fluid" alt="">
            </div>

            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center aos-init aos-animate" data-aos="zoom-in" data-aos-delay="300">
                <img src="https://bootstrapmade.com/demo/templates/Bethany/assets/img/clients/client-3.png" class="img-fluid" alt="">
            </div>

            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center aos-init aos-animate" data-aos="zoom-in" data-aos-delay="400">
                <img src="https://bootstrapmade.com/demo/templates/Bethany/assets/img/clients/client-4.png" class="img-fluid" alt="">
            </div>

            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center aos-init aos-animate" data-aos="zoom-in" data-aos-delay="500">
                <img src="https://bootstrapmade.com/demo/templates/Bethany/assets/img/clients/client-5.png" class="img-fluid" alt="">
            </div>

            <div class="col-lg-2 col-md-4 col-6 d-flex align-items-center aos-init aos-animate" data-aos="zoom-in" data-aos-delay="600">
                <img src="https://bootstrapmade.com/demo/templates/Bethany/assets/img/clients/client-6.png" class="img-fluid" alt="">
            </div>

        </div>

    </div>
</section>
